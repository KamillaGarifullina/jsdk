{
  "name": "@uploadcare/uc-blocks",
  "version": "0.0.6",
  "description": "Building blocks for Uploadcare products integration",
  "type": "module",
  "module": "index.js",
  "sideEffects": false,
  "repository": "https://github.com/uploadcare/uc-blocks/",
  "files": [
    "abstract/**/*.{js,css}",
    "blocks/**/*.{js,css}",
    "solutions/**/*.{js,css}",
    "submodules/**/*.{js,css}",
    "web/**/*.{js,css}",
    "index.js",
    "env.js",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "sym": "git submodule add -b main https://github.com/symbiotejs/symbiote.js submodules/symbiote",
    "update_submodules": "git submodule update --init --recursive --remote",
    "setup": "npm run update_submodules && npm i",
    "test": "test",
    "release": "shipjs prepare",
    "dev": "wds --open",
    "build": "npm run clean && node ./build.js",
    "tsc": "tsc --skipLibCheck --allowJs --noEmit",
    "lint": "eslint ./",
    "clean": "rimraf ./web/**/*.{js,css}"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@types/node": "^17.0.23",
    "@web/dev-server": "^0.1.30",
    "esbuild": "^0.14.21",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^1.3.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-jsdoc": "0.0.17",
    "rimraf": "^3.0.2",
    "shipjs": "^0.24.3",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "tsc --skipLibCheck --allowJs --noEmit",
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "Uploadcare Inc.",
  "license": "MIT"
}
